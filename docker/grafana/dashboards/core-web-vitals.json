{
  "dashboard": {
    "title": "Core Web Vitals - Linky Performance",
    "tags": ["performance", "web-vitals", "user-experience"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "type": "graph",
        "title": "Largest Contentful Paint (LCP)",
        "description": "Measures loading performance. Target: < 2.5s",
        "targets": [
          {
            "expr": "histogram_quantile(0.75, sum(rate(http_request_duration_seconds_bucket{metric=\"lcp\"}[5m])) by (le))",
            "legendFormat": "75th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{metric=\"lcp\"}[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "LCP (ms)"
          }
        ],
        "thresholds": [
          {
            "value": 2500,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          },
          {
            "value": 4000,
            "colorMode": "custom",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 2,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "type": "graph",
        "title": "First Input Delay (FID)",
        "description": "Measures interactivity. Target: < 100ms",
        "targets": [
          {
            "expr": "histogram_quantile(0.75, sum(rate(http_request_duration_seconds_bucket{metric=\"fid\"}[5m])) by (le))",
            "legendFormat": "75th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{metric=\"fid\"}[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "FID (ms)"
          }
        ],
        "thresholds": [
          {
            "value": 100,
            "colorMode": "critical",
            "op": "gt"
          },
          {
            "value": 300,
            "colorMode": "custom",
            "op": "gt"
          }
        ]
      },
      {
        "id": 3,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "type": "graph",
        "title": "Cumulative Layout Shift (CLS)",
        "description": "Measures visual stability. Target: < 0.1",
        "targets": [
          {
            "expr": "histogram_quantile(0.75, sum(rate(http_request_duration_seconds_bucket{metric=\"cls\"}[5m])) by (le))",
            "legendFormat": "75th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{metric=\"cls\"}[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "CLS Score"
          }
        ],
        "thresholds": [
          {
            "value": 0.1,
            "colorMode": "critical",
            "op": "gt"
          },
          {
            "value": 0.25,
            "colorMode": "custom",
            "op": "gt"
          }
        ]
      },
      {
        "id": 4,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "type": "graph",
        "title": "First Contentful Paint (FCP)",
        "description": "Measures perceived load speed. Target: < 1.8s",
        "targets": [
          {
            "expr": "histogram_quantile(0.75, sum(rate(http_request_duration_seconds_bucket{metric=\"fcp\"}[5m])) by (le))",
            "legendFormat": "75th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{metric=\"fcp\"}[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "FCP (ms)"
          }
        ],
        "thresholds": [
          {
            "value": 1800,
            "colorMode": "critical",
            "op": "gt"
          },
          {
            "value": 3000,
            "colorMode": "custom",
            "op": "gt"
          }
        ]
      },
      {
        "id": 5,
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 16 },
        "type": "singlestat",
        "title": "LCP - Good",
        "description": "Percentage of page loads with LCP < 2.5s",
        "targets": [
          {
            "expr": "sum(rate(http_request_duration_seconds_bucket{metric=\"lcp\",le=\"2.5\"}[5m])) / sum(rate(http_request_duration_seconds_count{metric=\"lcp\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "format": "percent",
        "colors": ["#d44a3a", "#e0b400", "#299c46"],
        "thresholds": "75,90",
        "valueName": "current"
      },
      {
        "id": 6,
        "gridPos": { "h": 4, "w": 6, "x": 6, "y": 16 },
        "type": "singlestat",
        "title": "FID - Good",
        "description": "Percentage of interactions with FID < 100ms",
        "targets": [
          {
            "expr": "sum(rate(http_request_duration_seconds_bucket{metric=\"fid\",le=\"0.1\"}[5m])) / sum(rate(http_request_duration_seconds_count{metric=\"fid\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "format": "percent",
        "colors": ["#d44a3a", "#e0b400", "#299c46"],
        "thresholds": "75,90",
        "valueName": "current"
      },
      {
        "id": 7,
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 16 },
        "type": "singlestat",
        "title": "CLS - Good",
        "description": "Percentage of page loads with CLS < 0.1",
        "targets": [
          {
            "expr": "sum(rate(http_request_duration_seconds_bucket{metric=\"cls\",le=\"0.1\"}[5m])) / sum(rate(http_request_duration_seconds_count{metric=\"cls\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "format": "percent",
        "colors": ["#d44a3a", "#e0b400", "#299c46"],
        "thresholds": "75,90",
        "valueName": "current"
      },
      {
        "id": 8,
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 16 },
        "type": "singlestat",
        "title": "Overall Performance Score",
        "description": "Percentage of page loads passing all Core Web Vitals",
        "targets": [
          {
            "expr": "(sum(rate(http_request_duration_seconds_bucket{metric=\"lcp\",le=\"2.5\"}[5m])) + sum(rate(http_request_duration_seconds_bucket{metric=\"fid\",le=\"0.1\"}[5m])) + sum(rate(http_request_duration_seconds_bucket{metric=\"cls\",le=\"0.1\"}[5m]))) / (sum(rate(http_request_duration_seconds_count{metric=\"lcp\"}[5m])) * 3) * 100",
            "refId": "A"
          }
        ],
        "format": "percent",
        "colors": ["#d44a3a", "#e0b400", "#299c46"],
        "thresholds": "75,90",
        "valueName": "current"
      },
      {
        "id": 9,
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 },
        "type": "table",
        "title": "Page Performance by URL",
        "targets": [
          {
            "expr": "topk(10, avg by (path) (rate(http_request_duration_seconds_sum{metric=\"lcp\"}[5m]) / rate(http_request_duration_seconds_count{metric=\"lcp\"}[5m])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "columns": [
          { "text": "URL", "value": "path" },
          { "text": "Avg LCP (ms)", "value": "Value" }
        ]
      },
      {
        "id": 10,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
        "type": "graph",
        "title": "Time to Interactive (TTI)",
        "description": "Measures time until page is fully interactive",
        "targets": [
          {
            "expr": "histogram_quantile(0.75, sum(rate(http_request_duration_seconds_bucket{metric=\"tti\"}[5m])) by (le))",
            "legendFormat": "75th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{metric=\"tti\"}[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "TTI (ms)"
          }
        ]
      },
      {
        "id": 11,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
        "type": "graph",
        "title": "Total Blocking Time (TBT)",
        "description": "Sum of blocking time for long tasks",
        "targets": [
          {
            "expr": "histogram_quantile(0.75, sum(rate(http_request_duration_seconds_bucket{metric=\"tbt\"}[5m])) by (le))",
            "legendFormat": "75th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{metric=\"tbt\"}[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "TBT (ms)"
          }
        ]
      },
      {
        "id": 12,
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 36 },
        "type": "graph",
        "title": "Performance Trends Over Time",
        "targets": [
          {
            "expr": "avg(rate(http_request_duration_seconds_sum{metric=\"lcp\"}[1h]) / rate(http_request_duration_seconds_count{metric=\"lcp\"}[1h]))",
            "legendFormat": "LCP",
            "refId": "A"
          },
          {
            "expr": "avg(rate(http_request_duration_seconds_sum{metric=\"fid\"}[1h]) / rate(http_request_duration_seconds_count{metric=\"fid\"}[1h]))",
            "legendFormat": "FID",
            "refId": "B"
          },
          {
            "expr": "avg(rate(http_request_duration_seconds_sum{metric=\"cls\"}[1h]) / rate(http_request_duration_seconds_count{metric=\"cls\"}[1h]))",
            "legendFormat": "CLS",
            "refId": "C"
          }
        ]
      }
    ]
  }
}
